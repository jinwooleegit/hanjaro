{
  "name": "hanjaro",
  "version": "0.1.0",
  "private": true,
  "license": "CC-BY-NC-4.0",
  "scripts": {
    "clean": "rimraf .next",
    "dev": "npm run clean && cross-env NODE_OPTIONS=\"--max_old_space_size=4096\" next dev -H 0.0.0.0",
    "dev:custom": "npm run clean && cross-env NODE_OPTIONS=\"--max_old_space_size=4096 --no-warnings\" next dev -p 3009",
    "dev:safe": "npm run clean && cross-env NODE_NO_WARNINGS=1 next dev -p 3010",
    "dev:learn": "npm run clean && cross-env NODE_OPTIONS=\"--max_old_space_size=4096\" NEXT_TELEMETRY_DISABLED=1 NODE_ENV=development next dev -p 3012",
    "build": "npm run clean && cross-env NODE_OPTIONS=\"--max_old_space_size=4096\" next build",
    "start": "next start",
    "lint": "next lint",
    "setup-auth": "node setup-auth.js",
    "normalize-data": "node scripts/normalize-data.js",
    "validate-data": "npx ts-node scripts/validate-data.ts",
    "fix-data": "npm run normalize-data && npm run validate-data",
    "generate-icons": "node scripts/generate-hanja-icons.js"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "autoprefixer": "^10.4.17",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.4.8",
    "critters": "^0.0.23",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "framer-motion": "^11.18.2",
    "hanzi-writer": "^3.7.2",
    "iconv-lite": "^0.6.3",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "mongodb": "^6.3.0",
    "mongoose": "^8.1.3",
    "next": "^14.1.0",
    "next-auth": "^4.24.11",
    "next-themes": "^0.2.1",
    "pdfkit-next": "^0.11.0",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "rimraf": "^5.0.10",
    "sharp": "^0.34.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@types/pdfjs-dist": "^2.10.377",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.2",
    "cross-env": "^7.0.3",
    "ignore-loader": "^0.1.2"
  }
}
